  "bugs": {
    "url": "https://github.com/tomgrooffer/gulp-sass-glob/issues"
  },
  "homepage": "https://github.com/tomgrooffer/gulp-sass-glob#readme",
  "main": "./dist/index.js",
  "scripts": {
